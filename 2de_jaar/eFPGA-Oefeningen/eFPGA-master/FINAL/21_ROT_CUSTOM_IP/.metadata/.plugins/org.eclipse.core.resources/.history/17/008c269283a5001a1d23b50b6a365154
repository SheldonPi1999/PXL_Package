#include <stdio.h>
#include "platform.h"
#include "xil_printf.h"

#include "ROTARY_ENCODER.h"

int main()
{
	int result;

    init_platform();

    print("Hello Mothefucka!\n\r");

    for(;;)
    {
    	result = ROTARY_ENCODER_mReadReg(XPAR_ROTARY_ENCODER_0_S00_AXI_BASEADDR, ROTARY_ENCODER_S00_AXI_SLV_REG2_OFFSET);
    	 result = AXI_CALCULATOR_mReadReg(XPAR_AXI_CALCULATOR_0_S00_AXI_BASEADDR, AXI_CALCULATOR_S00_AXI_SLV_REG2_OFFSET);
        printf("\nResult: %d", result);
    }

    cleanup_platform();
    return 0;
}
